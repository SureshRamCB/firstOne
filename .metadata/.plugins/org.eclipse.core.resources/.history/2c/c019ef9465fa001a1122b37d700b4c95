package demoPackage;

import org.testng.annotations.Test;

public class Groups {
	@Test(priority = -2)
	public void m01() {
		System.out.println("1");
	}
	@Test(priority = -5)
	public void m02() {
		System.out.println("2");
	}
	
	@Test(alwaysRun = true)
	public void m03() {
		System.out.println("3");
	}
	@Test(alwaysRun = false)
	public void m04() {
		System.out.println("4");
	}
}
