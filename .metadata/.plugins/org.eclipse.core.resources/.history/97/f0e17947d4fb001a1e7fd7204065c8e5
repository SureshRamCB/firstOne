public class Base {

	public static void main(String[] args) {
		Base.mu();
	}
	public static void mu() {
		System.setProperty("webdriver.chrome.drive","C:\\Users\\Admin\\Downloads\\chromedriver_win32\\chromedriver.exe");
		WebDriver driver=new ChromeDriver();
		driver.get("https://google.com");
	}
}
